@model  string[,]
<div class="card">
    <div class="card-header text-center"><h5>Datos Leídos</h5></div>
    <div class="card-body">
        <table class="table">
            <thead>
                <tr>
                    @for (int i = 0; i < Model.GetLength(1); i++)
                    {
                      <th scope="col">@Model[0, i]</th>
                    }
                </tr>
            </thead>
            <tbody>

                @for (int i = 1; i < Model.GetLength(0); i++)
                {
                    <tr>
                        @for (int j = 0; j < Model.GetLength(1); j++)
                        {
                        <td>@Model[i, j]</td>
                        }
                    </tr>
                }

            </tbody>

        </table>
        </div>
</div>
